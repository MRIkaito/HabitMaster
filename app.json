{
  "expo": {
    "name": "HabitMaster",
    "slug": "HabitMaster",
    "version": "1.0.0",
    "scheme": "HabitMaster",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.muran-codes.habitmaster",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.muran-codes.habitmaster",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-tracking-transparency"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8b0f5b0f-2742-4276-a461-a18482572922"
      }
    }
  }
}
